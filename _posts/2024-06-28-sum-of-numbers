#include <cs50.h>
#include <stdio.h>

int main(void)
{
    int number1;
    int number2;
    int number3;
    int sum;

    number1 = get_int("Enter the first number: ");
    number2 = get_int("Enter the second number: ");
    number3 = get_int("Enter the third number: ");
     sum = number1 + number2 + number3;

    printf("The sum of %i, %i, %i and %i is %i/ ./n", number1, number2, number3, sum);
}
